{
  "root": true,
  "extends": [
    "airbnb",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": 1,
    "trailing-comma": [false, {}],
    "comma-dangle": ["off"],
    "padded-blocks": ["error", "always"]
  }
}
